x = audioread('trumpet.wav');
[y,Fs] = audioread('trumpet.wav');
[y1,Fs1] = audioread('telephoneIR.wav');
info = audioinfo('trumpet.wav');
t1 = length(y1)/Fs1;
T = 0:1/Fs1:t1;
T = T(1:end-1);
t = 0:seconds(1/Fs):seconds(info.Duration);
t = t(1:end-1);
figure;
subplot(1,2,1)
plot(t,y);
xlabel('time')
ylabel('Audio signal');
subplot(1,2,2)
plot(T,y1)
xlabel('time')
ylabel('Audio signal');
